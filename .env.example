# ClaudeGen Coach - Environment Variables
# Copy this file to .env and fill in your actual values
# NEVER commit .env to git!

# ===========================
# Supabase (Required for cloud sync)
# ===========================
# Get from: https://supabase.com/dashboard/project/YOUR_PROJECT/settings/api
VITE_SUPABASE_URL=https://xxxxxxxxxxxxx.supabase.co
VITE_SUPABASE_ANON_KEY=eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9...

# ===========================
# Claude API (Required for AI features)
# ===========================
# Get from: https://console.anthropic.com/
VITE_CLAUDE_API_KEY=sk-ant-api03-xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx

# ===========================
# n8n Configuration (Optional)
# ===========================
# For workflow automation features
VITE_N8N_BASE_URL=https://your-n8n-instance.com
VITE_N8N_API_KEY=your_n8n_api_key

# ===========================
# Application Configuration
# ===========================
VITE_APP_ID=claudegen-coach-default

# ===========================
# Telegram Bot (Future feature)
# ===========================
# For bot service only (not used in web app)
TELEGRAM_BOT_TOKEN=your_telegram_bot_token
TELEGRAM_USER_ID=your_telegram_user_id
